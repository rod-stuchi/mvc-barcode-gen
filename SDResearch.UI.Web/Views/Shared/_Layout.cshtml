<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="Gerador randômico de código de barras" />
    <link rel="shortcut icon" href="/favicon.ico" />
    <title>@ViewBag.Title - Barcode Gen</title>
    <link href="~/Static/font-awesome/font-awesome-4.3.0/css/font-awesome.min.css" rel="stylesheet" />
    @Styles.Render("~/bundles/vendor-css")
    @Styles.Render("~/bundles/app-base-css")
    @RenderSection("css", false)
    <style>
    </style>
</head>

<body style="min-width:350px;">
    <nav class="navbar navbar-default nav-stu navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand" style="padding:0; margin:0 20px 0 0;" href="/Home/Index"><img class="img-responsive" src="~/Static/images/Barcodes-icon.png" width="48" , height="48" alt="Barcode Gen" title="Barcode Gen" /></a>
            </div>

            <div id="navbar" class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li><a href="/"><i class="fa fa-home fa-lg"></i></a></li>
                    <li><a href="/Boleto/Barcode">Boleto Bancário</a></li>
                    <li><a href="/ContaConsumo/Barcode">Conta de Consumo</a></li>

                </ul>
                <ul class="nav navbar-nav navbar-right" style="margin-right:2px;">
                    <li class="dropdown">
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false"><i class="fa fa-plus-square fa-lg"></i> <span class="caret"></span></a>
                        <ul class="dropdown-menu dropdown-menu-right">
                            <li><a href="/Extra/ByLine"><i class="fa fa-barcode fa-fw"></i> Gerar a partir da linha</a></li>
                            <li role="separator" class="divider"></li>
                            <li class="dropdown-header"><strong>Gerar código inválido</strong></li>
                            <li><a href="/Extra/InvalidBoleto"><i class="fa fa-exclamation-triangle fa-fw"></i> Boleto Bancário</a></li>
                            <li><a href="#"><i class="fa fa-exclamation-triangle fa-fw"></i> Conta Consumo</a></li>
                            <li role="separator" class="divider"></li>
                            <li class="dropdown-header">Gerar em lote</li>
                            <li><a href="#"><i class="fa fa-user fa-fw"></i> CPF</a></li>
                            <li><a href="#"><i class="fa fa-files-o fa-fw"></i> Boleto Bancário</a></li>
                            <li><a href="#"><i class="fa fa-files-o fa-fw"></i> Conta de consumo</a></li>
                        </ul>
                    </li>
                    <li><a href="#">Sobre</a></li>
                </ul>
            </div>

        </div>
    </nav>

    <div class="container">
        <div class="jumbotron">
            @RenderBody()
        </div>
    </div>

    @Scripts.Render("~/bundles/vendor-js")
    @Scripts.Render("~/bundles/app-base-js")
    @RenderSection("script", false)
</body>
</html>